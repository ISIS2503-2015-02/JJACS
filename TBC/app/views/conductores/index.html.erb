<h1>Lista de conductores</h1>

<table>
  <thead>
  <tr>
    <th>Nombre</th>
    <th>Cedula</th>
    <th>Puntaje</th>
    <th>Mobibus</th>
    <th>id</th>
    <th colspan="4"></th>
  </tr>
  </thead>

  <tbody>
  <% @conductores.each do |conductor| %>
      <tr>
        <td><%= conductor.nombre %></td>
        <td><%= conductor.cedula %></td>
        <td><%= conductor.puntaje %></td>
        <td><%= conductor.id %></td>
        <td><%= conductor.mobibus_id %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>

