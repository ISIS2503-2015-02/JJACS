<h1>Lista de Vcubs</h1>

<table>
  <thead>
  <tr>
    <th>Id Vcub</th>
    <th>Prestada</th>
    <th>Usuario Actual</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @vcubs.each do |vcub| %>
      <tr>
        <td><%=vcub.numero_identificacion %></td>
        <td><%=vcub.prestada %></td>
        <td><%=vcub.id_usuario_actual %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<br>
